@article{robinson_plastic_1967,
  title = {Plastic Deformation of Anthracene Single Crystals},
  volume = {15},
  issn = {0001-6160},
  abstract = {Tensile tests have been carried out on single crystals of anthracene over a range of temperatures and strain rates. A yield point is observed and plastic deformation occurs by slip in either the [010] or [110] directions in the basal plane. The yield point appears to be due to an initial difficulty in dislocation generation followed by a rapid multiplication of dislocations on yielding, rather than an impurity-locking effect. The critical shear stress for yielding and the appearance or non-appearance of the yield point are dependent on the surface finish of the crystals, presumably because the active dislocation sources are mainly at the surface. At temperatures less than T/TM = 0.67 for slip on (001) [010] the Peierls force appears to be a major factor in determining the critical shear stress for plastic deformation to begin, while for slip on (001) [110] the stress field due to defects probably makes a major contribution. On a effectu{\'e} des essais de traction sur des monocristaux d'anthrac{\`e}ne dans un certain domaine de temp{\'e}ratures et de vitesses de d{\'e}formation. On observe un crochet de traction; la d{\'e}formation plastique se produit par glissement dans l'une ou l'autre des directions [010] ou [110] dans le plan de base. L'origine du crochet de traction semble {\^e}tre une difficult{\'e} initiale dans la production des dislocations suivie d'une multiplication rapide des dislocations au coure de la d{\'e}formation plastique, plut{\^o}t qu'un effet de blocage par les impuret{\'e}s. La cission critique de d{\'e}formation plastique et l'apparition ou la non apparition du crochet de traction d{\'e}pendent du fini de la surface des cristaux, probablement parce que les sources actives de dislocations se trouvent principalement {\`a} la surface. A des temp{\'e}ratures inf{\'e}rieures {\`a} T/TM = 0,67, pour le glissement sur (001)[010], la force de Peierls semble {\^e}tre un facteur essentiel dans la d{\'e}termination de la cission critique n{\'e}cessaire pour provoquer la d{\'e}formation plastique, tandis que pour le glissement sur (001)[110], c'est probablement la champ de contraintes d{\`u} aux d{\'e}fauts qui donne une contribution pr{\'e}pond{\'e}rante. An Anthrazen-Einkristallen wurden bei verschiedenen Temperaturen und Verformungsgeschwindigkeiten Zugversuche durchgef{\"u}hrt. Es wird eine Streckgrenze beobachtet, und die plastische Verformung erfolgt durch Abgleitung in [010]- oder [110]-Richtung in der Basisebene. Die Streckgrenze scheint auf eine anf{\"a}ngliche Schwierigkeit bei der Versetzungserzeugung und nachfolgender rascher Versetzungsvervielfachung nach dem Flie{$\beta$}en zur{\"u}ckzuf{\"u}hren sein und weniger auf eine Verankerung durch Verunreinigungen. Die kritische Schubspannung und das Auftreten oder Nichtauftreten der Streckgrenze h{\"a}ngen von der Oberfl{\"a}chenbehandlung des Kristalls ab, wahrscheinlich deshalb, weil die aktiven Versetzungsquellen haupts{\"a}chlich an der Oberfl{\"a}che liegen. Bei Temperaturen unterhalb T/TM = 0,67 scheint bei Abgleitung auf (001)[010] die Peierlsspannung ein wesentlicher Faktor f{\"u}r die Bestimmung der kritischen Schubspannung zu sein, w{\"a}hrend bei Abgleitung auf (001)[110] das Spannungsfeld von Gitterfehlern wahrscheinlich einen gr{\"o}{$\beta$}eren Beitrag liefert.},
  number = {10},
  journal = {Acta Metallurgica},
  doi = {10.1016/0001-6160(67)90131-9},
  author = {Robinson, P. M and Scott, H. G},
  month = oct,
  year = {1967},
  pages = {1581-1590},
  file = {/home/ken/Zotero/storage/SLBW4VY3/Robinson and Scott - 1967 - Plastic deformation of anthracene single crystals.pdf}
}

